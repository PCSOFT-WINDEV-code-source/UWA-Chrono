   -
     name : Durée
     procedure_id : 1686509824642839193
     type_code : 14
     code : |1+
      //Affiche une durée sous forme d'aiguille analogique dans un champ image
      // Syntaxe :
      //Durée ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      
      Procédure Durée()
      
      nXCentre, nYCentre			sont des entiers				//Coordonnées du centre du chrono
      sNomChampHeure				est une chaine = IMG_Heure..nom	//Nom du champ qui va afficher les aiguilles
      nLongueurAiguilleMinutes	est un entier					//Longueur de l'aiguille des minutes
      duDurée						est une duree 					//Durée (différence) à afficher
      
      si gbPause alors
      	retour
      FIN
      
      //Calcul des coordonnées du centre du chrono	
      nXCentre = {sNomChampHeure,indChamp}..Largeur/2
      nYCentre = ({sNomChampHeure,indChamp}..hauteur+20)/2 
      
      //Calcul de la longueur de l'aiguille des minutes (marge de 30 sur le bord le plus petit)
      nLongueurAiguilleMinutes = min({sNomChampHeure,indChamp}..Largeur/2,{sNomChampHeure,indChamp}..hauteur/2)-30
      
      //Nouveau dessin
      dDebutDessin(sNomChampHeure,dEffacer)
      
      //Durée écoulée
      duDurée = ChronoValeur(1)
      
      //Déduit la durée à ne pas comptabiliser
      duDurée = duDurée - gduTempsNonCompté
      
      //Dessin des aiguilles
      DessineHeures(duDurée, nLongueurAiguilleMinutes, nXCentre, nYCentre)
      DessineMinutes(duDurée, nLongueurAiguilleMinutes, nXCentre, nYCentre)
      DessineSecondes(duDurée, nLongueurAiguilleMinutes, nXCentre, nYCentre)
      
      //Affichage de la durée écoulée	
      SAI_Durée = DuréeVersChaîne(duDurée, "HH:MM:SS")
     type : 458752
   -
     name : DessineHeures
     procedure_id : 1686544510798410189
     type_code : 14
     code : |1-
      // Résumé : Dessine l'aiguille des heures
      // Syntaxe :
      //DessineHeures (<duDurée>, <nLongueurAiguilleMinutes>, <nXCentre>, <nYCentre>)
      //
      // Paramètres :
      //	duDurée : Durée à représenter
      //	nLongueurAiguilleMinutes : Longueur de la plus grande aiguille, celle des minutes
      //	nXCentre : Abscisse du centre du chrono
      //	nYCentre : Ordonnée du centre du chrono
      // Valeur de retour :
      // 	Aucune
      //
      
      Procédure DessineHeures(duDurée, nLongueurAiguilleMinutes, nXCentre, nYCentre)
      
      nAngle					est un reel						//Angle pour l' aiguille
      nCouleurAiguille 		est un entier = RougeFoncé		//Couleur de l'aiguille
      nLongueurAiguilleHeures est un entier 					//Longueur de l'aiguille des heures
      rX, rY 					sont des reels					//Coordonnées du bout de l'aiguille
      
      //Pour l'aiguille des heures : modulo 12, et produit en croix de 12 par rapport à 360 (360°)
      //+ 90° pour aller à 12h
      nAngle = modulo(duDurée..Heure,12)*360/12+90
      
      //Aiguille des heures légèrement plus courte que celle des minutes
      nLongueurAiguilleHeures = nLongueurAiguilleMinutes-20
      
      //Calcul des coordonnées du bout de l'aiguille
      rX = nLongueurAiguilleHeures*Cosinus(nAngle) 
      rY = nLongueurAiguilleHeures*Sinus(nAngle)
      
      // Dessin du trait de l'aiguille des heures (épaisseur 4)	
      dLigne(nXCentre, nYCentre, nXCentre - rX, nYCentre - rY, nCouleurAiguille, 7)
     type : 458752
   -
     name : DessineMinutes
     procedure_id : 1686545215173144764
     type_code : 14
     code : |1-
      // Résumé : Dessine l'aiguille des minutes
      // Syntaxe :
      //DessineMinutes (<duDuree>, <nLongueurAiguilleMinutes>, <nXCentre>, <nYCentre>)
      //
      // Paramètres :
      //	duDuree : Durée à représenter
      //	nLongueurAiguilleMinutes : Longueur de la plus longue aiguille, celle des minutes
      //	nXCentre : Abscisse du centre du chrono
      //	nYCentre : Ordonnée du centre du chrono
      // Valeur de retour :
      // 	Aucune
      
      Procédure DessineMinutes(duDuree ,nLongueurAiguilleMinutes, nXCentre, nYCentre)
      
      nAngle					est un reel						//Angle pour l' aiguille des minutes
      nCouleurAiguille 		est un entier = RougeFoncé		//Couleur de l'aiguille
      rX, rY 					sont des reels					//Coordonnées du bout de l'aiguille
      
      //Pour l'aiguille des minutes : produit en croix de 60 par rapport à 360 (360°)
      //+ 90° pour aller à 12h	
      nAngle = duDuree..Minute*360/60+90
      
      //Calcul des coordonnées du bout de l'aiguille
      rX = nLongueurAiguilleMinutes*Cosinus(nAngle) 
      rY = nLongueurAiguilleMinutes*Sinus(nAngle)
      
      //Dessin du trait de l'aiguille des minutes (épaisseur 2)	
      dLigne(nXCentre , nYCentre , nXCentre - rX, nYCentre - rY , nCouleurAiguille,4)
     type : 458752
   -
     name : DessineSecondes
     procedure_id : 1686546061281767893
     type_code : 14
     code : |1-
      // Résumé : Dessine l'aiguille des secondes
      // Syntaxe :
      //DessineSecondes (<duDurée>, <nLongueurAiguilleMinutes>, <nXCentre>, <nYCentre>)
      //
      // Paramètres :
      //	duDurée :  Durée à représenter
      //	nLongueurAiguilleMinutes : Longueur de la plus longue aiguille, celle des minutes
      //	nXCentre : Abscisse du centre du chrono
      //	nYCentre : Ordonnée du centre du chrono
      // Valeur de retour :
      // 	Aucune
      
      
      Procédure DessineSecondes(duDurée, nLongueurAiguilleMinutes, nXCentre, nYCentre)
      
      nAngle					est un reel						//Angle pour l' aiguille
      nCouleurAiguille 		est un entier = RougeFoncé		//Couleur de l'aiguille
      rX, rY 					sont des reels					//Coordonnées du bout de l'aiguille
      
      
      //Pour l'aiguille des secondes : produit en croix de 60 par rapport à 360 (360°)
      //+ 90° pour aller à 12h	
      nAngle = duDurée..Seconde*360/60+90
      
      //Calcul des coordonnées du bout de l'aiguille
      //NB : Longueur de l'aiguille des secondes identique à celle des minutes
      rX = nLongueurAiguilleMinutes*Cosinus(nAngle) 
      rY = nLongueurAiguilleMinutes*Sinus(nAngle)
      
      //Dessin du trait de l'aiguille des secondes (épaisseur 1)
      dLigne(nXCentre , nYCentre , nXCentre - rX, nYCentre - rY, nCouleurAiguille, 2)
     type : 458752
   -
     name : InitialiseAiguilles
     procedure_id : 1692508704255184582
     type_code : 14
     code : |1-
      // Résumé : Dessine les aiguilles lorsqu'il n'y a encore aucune durée calculée
      // Syntaxe :
      //InitialiseAiguilles ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      
      Procédure InitialiseAiguilles()
      
      nXCentre, nYCentre			sont des entiers				//Coordonnées du centre du chrono
      sNomChampHeure				est une chaine = IMG_Heure..nom	//Nom du champ qui va afficher les aiguilles
      nLongueurAiguilleMinutes	est un entier					//Longueur de l'aiguille des minutes
      duDurée est une durée
      
      //Calcul des coordonnées du centre du chrono	
      nXCentre = {sNomChampHeure,indChamp}..Largeur/2
      nYCentre = ({sNomChampHeure,indChamp}..Hauteur+20)/2 
      
      //Calcul de la longueur de l'aiguille des minutes (marge de 30 sur le bord le plus petit)
      nLongueurAiguilleMinutes = Min({sNomChampHeure,indChamp}..Largeur/2,{sNomChampHeure,indChamp}..Hauteur/2)-30
      
      //Nouveau dessin
      dDébutDessin(sNomChampHeure,dEffacer)
      
      //Dessin des aiguilles 
      dessineHeures(duDurée, nLongueurAiguilleMinutes, nXCentre, nYCentre)
      DessineMinutes(duDurée, nLongueurAiguilleMinutes, nXCentre, nYCentre)
      DessineSecondes(duDurée, nLongueurAiguilleMinutes, nXCentre, nYCentre)
     type : 458752
